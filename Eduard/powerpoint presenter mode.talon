app: Microsoft powerpoint
mode: sleep
-
[<phrase>] next [<phrase>]:
    key(right)
    
[<phrase>] forward [<phrase>]:
    key(right)

[<phrase>] aloft [<phrase>]:
    key(right)
    
[<phrase>] go back [<phrase>]:
    key(left)
    
[<phrase>] jump <user.number_string>:
    "{number_string}"
    key(enter)

